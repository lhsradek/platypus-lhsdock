# Module: enterprisesearch
# Docs: https://www.elastic.co/guide/en/beats/metricbeat/8.4/metricbeat-module-enterprisesearch.html

- module: enterprisesearch
  metricsets: ["health", "stats"]
  enabled: true
  period: 10s
  hosts: ["http://eps.nginx.local:3002"]
  username: "${ELASTICSEARCH_USERNAME}"
  password: "${ELASTICSEARCH_PASSWORD}"
  # username: "admin"
  # password: "${EPS_PASSWORD}"
